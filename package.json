{
  "name": "dynolink",
  "version": "1.0.0",
  "description": "A TypeScript ORM for DynamoDB",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
